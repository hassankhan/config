{
    "name": "mhndev/config",
    "type": "library",
    "description": "Lightweight configuration file loader that supports PHP, INI, XML, JSON, and YAML files forked from hassankhan/config",
    "keywords": ["configuration", "config", "json", "yaml", "yml", "ini", "xml", "unframework", "microphp"],
    "homepage": "https://github.com/mhndev/config",
    "license": "MIT",
    "authors": [
        {
            "name": "Majid Abdolhosseini",
            "email": "majid8911303@gmail.com",
            "homepage": "http://www.mhndev.com/"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.2"
    },
    "suggest": {
        "symfony/yaml": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "mhndev\\config\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mhndev\\config\\Test\\": "tests"
        }
    }
}
